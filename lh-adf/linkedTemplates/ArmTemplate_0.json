		{
			"name": "[concat(parameters('factoryName'), '/employee_flowlet')]",
			"type": "Microsoft.DataFactory/factories/dataflows",
			"apiVersion": "2018-06-01",
			"properties": {
				"type": "Flowlet",
				"typeProperties": {
					"sources": [],
					"sinks": [],
					"transformations": [
						{
							"name": "upperName"
						},
						{
							"name": "selectCols"
						},
						{
							"name": "employee"
						},
						{
							"name": "output1"
						}
					],
					"scriptLines": [
						"input(output(",
						"          empID as string,",
						"          name as string,",
						"          country as string,",
						"          deptID as string",
						"     ),",
						"     order: 0,",
						"     allowSchemaDrift: true) ~> employee",
						"employee derive(name = upper(name)) ~> upperName",
						"upperName select(mapColumn(",
						"          empID,",
						"          name,",
						"          deptID",
						"     ),",
						"     skipDuplicateMapInputs: true,",
						"     skipDuplicateMapOutputs: true) ~> selectCols",
						"selectCols output() ~> output1"
					]
				}
			},
			"dependsOn": []
		},
		{
			"name": "[concat(parameters('factoryName'), '/func_upperString')]",
			"type": "Microsoft.DataFactory/factories/dataflows",
			"apiVersion": "2018-06-01",
			"properties": {
				"type": "UDFLibrary",
				"typeProperties": {
					"sources": [],
					"sinks": [],
					"transformations": [],
